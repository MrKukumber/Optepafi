<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:moVm="clr-namespace:Optepafi.ViewModels"
                    xmlns:mapObjects="clr-namespace:Optepafi.ViewModels.Graphics.MapObjects">
    <DataTemplates x:Key="TextMapObjectTemplates">
        <DataTemplate DataType="{x:Type mapObjects:WordObjectViewModel}">
            <TextBlock Text="{Binding Word}"/>
        </DataTemplate>
    </DataTemplates>
</ResourceDictionary>
