<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:moVm="clr-namespace:Optepafi.ViewModels.Data.Graphics.MapObjects">
    <DataTemplates x:Key="TextMapObjectTemplates">
        <DataTemplate DataType="{x:Type moVm:WordObjectViewModel}">
            <TextBlock Text="{Binding Word}"/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type moVm:TrackPointWordObjectViewModel}">
            <TextBlock Text="o" Foreground="Red"></TextBlock>
        </DataTemplate>
    </DataTemplates>
</ResourceDictionary>
